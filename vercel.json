{
    "functions": {
        "api/*.js": {
          "timeout": 40
        }
      },
    "version": 2,
    "builds": [
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/socket.io/*",
        "dest": "server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "build/$1"
      }
    ]
  }